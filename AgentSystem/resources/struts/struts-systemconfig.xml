		<action name="addconfig" class="org.agent.action.SystemConfigAction" method="addConfig"></action>
		<action name="deleteconfig" class="org.agent.action.SystemConfigAction" method="deleteConfig"></action>
		<action name="modifyconfig" class="org.agent.action.SystemConfigAction" method="modifyConfig"></action>
	</package>
