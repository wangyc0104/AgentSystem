<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd" >
<struts>
	<package name="systemconfig" namespace="/" extends="admin">
		<action name="caiwutype" class="org.agent.action.SystemConfigAction" method="caiwuType">
			<result>/WEB-INF/pages/systemconfig.jsp</result>
		</action>
		<action name="servicetype" class="org.agent.action.SystemConfigAction" method="serviceType">
